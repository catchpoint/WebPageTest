# Doxyfile 1.5.4

PROJECT_NAME           = Botan
PROJECT_NUMBER         = %{version}
OUTPUT_DIRECTORY       = %{doc_output_dir}/doxygen
DOXYFILE_ENCODING      = UTF-8
CREATE_SUBDIRS         = NO
OUTPUT_LANGUAGE        = English
BRIEF_MEMBER_DESC      = YES
REPEAT_BRIEF           = YES
ABBREVIATE_BRIEF       =
ALWAYS_DETAILED_SEC    = NO
INLINE_INHERITED_MEMB  = YES
FULL_PATH_NAMES        = YES
STRIP_FROM_PATH        =
